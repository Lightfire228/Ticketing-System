@model IEnumerable<TicketApp.Models.Ticket>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Tickets</h2>
<!--add ticket component statement to this maybe? or just redirect to ticket body page-->
<ul>
    @foreach (var ticket in Model) {
        <li>@ticket.Title (@ticket.Status)</li>
    }
</ul>